---
import SanityMarkdown from '@app/components/astro/sanity/markdown.astro';
import type { CommonSchemaType } from '@app/types/content';

export type SanityMarkdown = CommonSchemaType<'markdown'>;

export interface Props extends SanityMarkdown {}

const props = Astro.props;

const { content, ...rest } = props;
---

<SanityMarkdown content={content} {...rest} />
